{
  "name": "ab-test-selector",
  "version": "2.0.0",
  "type": "module",
  "description": "Consistently assigns by argument to A or B if enabled, and set to ALL or argument in include list and not in exclude list",
  "main": "index.mjs",
  "scripts": {
    "watchUnit": "NODE_ENV=DEBUG ava --fail-fast -v **/__tests__/**/*test*.mjs --watch",
    "ava": "ava",
    "test": "ava --fail-fast -v __tests__/*tests.mjs",
    "watchLint": "esw . --ext=js --ext=mjs --ignore-path .gitignore --fix --watch",
    "lint": "eslint . --ext=js --ext=mjs --ignore-path .gitignore --fix"
  },
  "keywords": [
    "ab",
    "test",
    "abtest",
    "ab-test",
    "multitenant",
    "multi-tenant"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "esm": {
    "force": true,
    "mode": "auto"
  },
  "author": "Troy Collinsworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:tcollinsworth/ab-test-selector.git"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-watch": "^7.0.0",
    "ignore-styles": "^5.0.1"
  }
}
