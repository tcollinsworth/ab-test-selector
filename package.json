{
  "name": "ab-test-selector",
  "version": "3.0.2",
  "description": "Consistently assigns by argument to A or B test pool if enabled, also supports include and exclude lists",
  "main": "index.js",
  "scripts": {
    "watchUnit": "NODE_ENV=DEBUG ava --fail-fast -v **/__tests__/**/*test*.js --watch",
    "ava": "ava",
    "test": "ava --fail-fast -v __tests__/*tests.js",
    "watchLint": "esw . --ext=js --ignore-path .gitignore --fix --watch",
    "lint": "eslint . --ext=js --ignore-path .gitignore --fix"
  },
  "keywords": [
    "ab",
    "test",
    "abtest",
    "ab-test",
    "multitenant",
    "multi-tenant"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "esm": {
    "force": true,
    "mode": "auto"
  },
  "author": "Troy Collinsworth",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:tcollinsworth/ab-test-selector.git"
  },
  "dependencies": {
    "md5": "^2.3.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-watch": "^8.0.0",
    "esm": "^3.2.25",
    "ignore-styles": "^5.0.1"
  }
}
